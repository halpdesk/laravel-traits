{
    "name": "halpdesk/laravel-simple-stripe",
    "description": "Stripe service provider and migration files for Stripe payment API",
    "keywords": ["halpdesk", "laravel", "stripe"],
    "type": "library",
    "require": {
        "php": ">=7.3.4",
        "laravel/framework": "5.8.*",
        "stripe/stripe-php": "^6.39"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.7",
        "mockery/mockery": "^1.1",
        "absolutehh/dotenv-manipulator": "^1.2",
        "doctrine/dbal": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Halpdesk\\LaravelMinimumPackage\\": "src/",
            "Halpdesk\\LaravelMinimumPackage\\Tests\\": "tests/"
        },
        "files": [
        ],
        "classmap": [
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
