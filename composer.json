{
    "name": "halpdesk/minimum-package",
    "description": "Bare minimum to aid building composer packages for Laravel framework",
    "keywords": ["halpdesk", "laravel", "composer", "php"],
    "type": "library",
    "require": {
        "php": ">=7.3.4",
        "laravel/framework": "5.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.7",
        "mockery/mockery": "^1.1",
        "absolutehh/dotenv-manipulator": "^1.2",
        "doctrine/dbal": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Halpdesk\\LaravelMinimumPackage\\": "src/",
            "Halpdesk\\LaravelMinimumPackage\\Tests\\": "tests/"
        },
        "files": [
        ],
        "classmap": [
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
