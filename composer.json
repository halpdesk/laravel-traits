    "keywords": ["laravel", "traits", "camel", "filter", "transformers"],
    "type": "library",
    "require": {
        "php": ">=7.3.4",
        "laravel/framework": "5.8.*",
        "league/fractal": "^0.18.0",
        "halpdesk/helpers": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.7",
        "mockery/mockery": "^1.1",
        "absolutehh/dotenv-manipulator": "^1.2",
        "doctrine/dbal": "^2.9"
